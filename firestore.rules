rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /usuarios/{userId}/casos/{caseId} {
      allow read, create, update, delete: if request.auth != null && request.auth.uid == userId;

      match /montagens/{montagemId} {
        allow read, create, update, delete: if request.auth != null && request.auth.uid == userId;
      }
    }
  }
}
